{
  "name": "groton-school/slim-lti-shim",
  "authors": [
    {
      "name": "Seth Battis",
      "email": "sbattis@groton.org"
    }
  ],
  "license": "GPL-3.0",
  "require": {
    "delight-im/cookie": "^3.4",
    "groton-school/slim-action": "dev-main",
    "packbackbooks/lti-1p3-tool": "^6.2",
    "php-di/php-di": "^7.0",
    "slim/http": "^1.4",
    "slim/slim": "^4.14"
  },
  "autoload": {
    "psr-4": {
      "GrotonSchool\\Slim\\LTI\\": "src/"
    }
  },
  "scripts": {
    "release": "conventional-changelog --commit"
  },
  "require-dev": {
    "franzl/studio": "^0.16.0",
    "marcocesarato/php-conventional-changelog": "^1.17"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "franzl/studio": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
